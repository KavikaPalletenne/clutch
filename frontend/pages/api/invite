// Next.js API route support: https://nextjs.org/docs/api-routes/introduction
import type { NextApiRequest, NextApiResponse } from 'next'

type Data = {
  message: string
}

export default function handler(
  req: NextApiRequest,
  res: NextApiResponse<Data>
) {
  res.status(301).setHeader("Location", "https://discord.com/api/oauth2/authorize?client_id=917954795384500236&permissions=377989876800&scope=bot").json({ message: 'Discord Login' })
}
